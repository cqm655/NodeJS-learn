<%- include('../includes/head.ejs') %>
</head>

<body>
<%- include('../includes/navigation.ejs') %>
<main>
    <% if(orders.length <= 0 ) { %>
        <h1>Nothing there!</h1>
    <% } else { %>
        <ul>
            <% orders.forEach(order => { %>
                <li>
                    <h3># <%= order.id %></h3>
                    <ul>
                        <% order.products.forEach(item => { %>
                            <li><%= item.title %> (<%= item.orderItem.quantity %>)</li>
                        <% }); %>
                    </ul>
                </li>
            <% }); %>
        </ul>
    <% } %>

</main>
<%- include('../includes/end.ejs') %>
