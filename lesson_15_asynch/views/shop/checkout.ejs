<body>
<%- include('../includes/navigation.ejs') %>

<main>
    <ul class="cart__item-list centered">

        <% products.forEach(p => { %>
            <li class="product-list">
                <p>Title: <%= p.title %></p>
                <p>Quantity: <%= p.cartItem.quantity %></p>
            </li>
        <% }) %>

    </ul>
    <div>
        <h2>Total: <%= totalSum %></h2>
    </div>

</main>


<%- include('../includes/end.ejs') %>
